{
    "extends": [
        "eslint-config-appium"
    ],
    "plugins": ["jest"],
    "rules": {
        "no-console": "off",
        "no-control-regex": "off",
        "promise/no-native": "off",
        "promise/prefer-await-to-callbacks": "off",
        "promise/prefer-await-to-then": "off",
        "promise/catch-or-return": "off",
        "space-before-function-paren": "off"
    },
    "env": {
        "jest/globals": true
    }
}
